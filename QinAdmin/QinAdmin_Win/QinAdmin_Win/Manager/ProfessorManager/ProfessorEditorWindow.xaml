<Window x:Class="QinAdmin_Win.ProfessorManager.ProfessorEditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QinAdmin_Win.ProfessorManager"
        mc:Ignorable="d"
        Title="ProfessorEditorWindow" Height="420.577" Width="341.553" Loaded="Window_Loaded">
    <DockPanel LastChildFill="False" Margin="32">
        <Label Content="姓名" DockPanel.Dock="Top"/>
        <TextBox x:Name="ProfessorNameTextBox" DockPanel.Dock="Top" />
        <Label Content="学校编号" DockPanel.Dock="Top"/>
        <TextBox x:Name="SchoolIDTextBox" DockPanel.Dock="Top" />
        <Button x:Name="ConfirmButton" Content="确定" DockPanel.Dock="Bottom" Click="ConfirmButton_Click"/>
    </DockPanel>
</Window>

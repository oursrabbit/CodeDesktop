<Window x:Class="QinAdmin_Win.CourseManager.CourseEditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QinAdmin_Win.CourseManager"
        mc:Ignorable="d"
       Title="CourseEditorWindow" Height="263.207" Width="234.906" Loaded="Window_Loaded">
    <DockPanel Margin="32" LastChildFill="False">
        <Label Content="课程编号" DockPanel.Dock="Top"/>
        <TextBox x:Name="CourseIDTextBox" DockPanel.Dock="Top"/>
        <Label Content="课程名称" DockPanel.Dock="Top"/>
        <TextBox x:Name="CourseNameTextBox" DockPanel.Dock="Top"/>
        <Button x:Name="ConfirmButton" Content="确定" DockPanel.Dock="Bottom" Click="ConfirmButton_Click"/>
    </DockPanel>
</Window>
